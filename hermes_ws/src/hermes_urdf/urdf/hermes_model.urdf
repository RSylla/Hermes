<?xml version="1.0"?>
<robot name="hermes">

    <material name="grey">
        <color rgba="0.7 0.7 0.7 1"/>
    </material>

    <link name="base_footprint">
    	<origin xyz="0 0 0"/>
    </link>    
    
    
    <link name="base_link">
        <inertial>
            <mass value="25.0"/>
            <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <origin xyz="-0.00017382192779770617736 0.44233349576524649827 -0.0084281873210687308084" rpy="0 -0 -1.570796326794896558" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/kere.stl"/>
            </geometry>
            <material name="kere_material">
                <color rgba="0.61568627450980395466 0.81176470588235294379 0.92941176470588238168 1.0"/>
            </material>
        </visual>
        <collision>
        	<origin xyz="-0.00017382192779770617736 0.44233349576524649827 -0.0084281873210687308084" rpy="0 -0 -1.570796326794896558" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/kere.stl"/>
            </geometry>
        </collision>
    </link>

    <link name="rear_right_wheel">
        <inertial>
            <mass value="3.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv_tagumine.stl"/>
            </geometry>
            <material name="rehv1_material">
                <color rgba="0.61568627450980395466 0.81176470588235294379 0.92941176470588238168 1.0"/>
            </material>
        </visual>
        <collision>
        	<origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv_tagumine.stl"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_rear_right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="rear_right_wheel"/>
        <origin xyz="0.25732617807220242723 0.88155349576524644206 0.0015718126789312780733" rpy="1.570796326794896558 1.570796326794896558 0" />
        <axis xyz="0 -1 0"/>
    </joint>

    <link name="front_left_wheel">
        <inertial>
            <mass value="10.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 1.58" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv.stl"/>
            </geometry>
            <material name="rehv1_material">
                <color rgba="0.61568627450980395466 0.81176470588235294379 0.92941176470588238168 1.0"/>
            </material>
        </visual>
        <collision>
        	<origin xyz="0 0 0" rpy="0 0 1.58" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv.stl"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_front_left_wheel_joint" type="continuous">
        <parent link="liigend"/>
        <child link="front_left_wheel"/>
        <origin xyz="0.30257617807220232864 -0.00024642929502571853106 -0.0034281873210687194287" rpy="0 -0 3.141592653589793116" />
        <axis xyz=" -1 0 0" />
    </joint>

    <link name="rear_left_wheel">
        <inertial>
            <mass value="3.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv_tagumine.stl"/>
            </geometry>
            <material name="rehv1_material">
                <color rgba="0.61568627450980395466 0.81176470588235294379 0.92941176470588238168 1.0"/>
            </material>
        </visual>
        <collision>
        	<origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv_tagumine.stl"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_rear_left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="rear_left_wheel"/>
        <origin xyz="-0.207673821927797847 0.88155349576524655308 0.0015718126789312503178" rpy="1.570796326794896558 1.570796326794896558 0" />
        <axis xyz="0 -1 0" />
    </joint>
    
    <link name="front_right_wheel">
        <inertial>
            <mass value="10.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 1.583" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv.stl"/>
            </geometry>
            <material name="rehv1_material">
                <color rgba="0.61568627450980395466 0.81176470588235294379 0.92941176470588238168 1.0"/>
            </material>
        </visual>
        <collision>
        	<origin xyz="0 0 0" rpy="0 0 1.583" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/rehv.stl"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_front_right_wheel_joint" type="continuous">
        <parent link="liigend"/>
        <child link="front_right_wheel"/>
        <origin xyz="-0.30292382192779770955 -0.00024642929502571853106 -0.0034281873210687333065" rpy="0 -0 0" />
        <axis xyz="-1 0 0" />
    </joint>

    <link name="liigend">
        <inertial>
            <mass value="2.0"/>
            <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 -0 0" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/liigend.stl"/>
            </geometry>
            <material name="liigend_material">
                <color rgba="0.61568627450980395466 0.81176470588235294379 0.92941176470588238168 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="liigend_to_kere_joint" type="revolute">
        <parent link="base_link"/>
        <child link="liigend"/>
        <origin xyz="-0.00017382192779770433422 -0.00024642929502571853106 -0.0034281873210687263676" rpy="0 -0 0" />
        <axis xyz="0 0 1"/>
        <limit lower="-0.7854" upper="0.7854" effort="50" velocity="2"/>

        
    </joint>
    
    <joint name="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.1" rpy="0 0 0"/>
    </joint>

    
    <link name="imu_link">
    </link>

    <joint name="base_link_to_imu" type="fixed">
        <parent link="base_link"/>
        <child link="imu_link"/>
        <origin xyz="-0.00032976124091513292491 -0.091578898488961058 0.3865718126789312592" rpy="6.3108872417680804303e-30 -2.9537602700216738211e-30 -3.141592653589793116" />
    </joint>

    
    
    
    <ros2_control name="MyRobotHardware" type="system">

          <!-- Use the GenericSystem hardware plugin for real hardware -->
          <hardware>
              <plugin>ros2_control_components/GenericSystem</plugin>
              <param name="example_param">example_value</param>
          </hardware>

          <!-- Define the left wheel joint -->
          <joint name="base_rear_left_wheel_joint">
              <command_interface name="velocity">
                  <param name="min">-10</param>
                  <param name="max">10</param>
              </command_interface>
              <state_interface name="position"/>
              <state_interface name="velocity"/>
          </joint>

          <!-- Define the right wheel joint -->
          <joint name="base_rear_right_wheel_joint">
              <command_interface name="velocity">
                  <param name="min">-10</param>
                  <param name="max">10</param>
              </command_interface>
              <state_interface name="position"/>
              <state_interface name="velocity"/>
          </joint>
    
      </ros2_control>
    
    <material name="ld19 color">
        <color rgba="0.2313725490196078538 0.38039215686274507888 0.7058823529411765163 1.0"/>
    </material>
    
    <link name="ld19">
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.5" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="0.5"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://hermes_urdf/urdf/ld19.stl"/>
            </geometry>
            <material name="ld19_material">
                <color rgba="0.2313725490196078538 0.38039215686274507888 0.7058823529411765163 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="base_lidar_joint" type="fixed">
        <parent link="base_link"/>
        <child link="ld19"/>
        <origin xyz="-0.00032976124091513292491 -0.091578898488961058 0.3865718126789312592" rpy="6.3108872417680804303e-30 -2.9537602700216738211e-30 -3.141592653589793116" />

    </joint>
    
    
    <transmission name="front_left_wheel_transmission">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="front_left_wheel_joint"/>
      <actuator name="front_left_wheel_motor"/>
      <mechanicalReduction>1.0</mechanicalReduction>  <!-- Adjust if you have gearing -->
    </transmission>

    <transmission name="front_right_wheel_transmission">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="front_right_wheel_joint"/>
      <actuator name="front_right_wheel_motor"/>
      <mechanicalReduction>1.0</mechanicalReduction>
    </transmission>



    
</robot>



