<?xml version="1.0"?>
<robot name="two_motor_robot">

  <!-- Define a fixed link for the base -->
  <link name="base_link"/>

  <!-- Define a link for the fifth wheel-like axle -->
  <link name="fifth_wheel_axle">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- Define a link for the left motor -->
  <link name="left_motor">
    <!-- Add visual and collision elements for left motor -->
  </link>

  <!-- Define a link for the right motor -->
  <link name="right_motor">
    <!-- Add visual and collision elements for right motor -->
  </link>

  <!-- Define joints -->

  <!-- Joint between base and fifth wheel-like axle -->
  <joint name="base_to_fifth_wheel_axle" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="fifth_wheel_axle"/>
  </joint>

  <!-- Joint between left motor and fifth wheel-like axle -->
  <joint name="left_motor_to_fifth_wheel_axle" type="continuous">
    <!-- Adjust origin and axis based on your specific setup -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="left_motor"/>
    <child link="fifth_wheel_axle"/>
    <axis xyz="1 0 0"/>
  </joint>

  <!-- Joint between right motor and fifth wheel-like axle -->
  <joint name="right_motor_to_fifth_wheel_axle" type="continuous">
    <!-- Adjust origin and axis based on your specific setup -->
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="right_motor"/>
    <child link="fifth_wheel_axle"/>
    <axis xyz="1 0 0"/>
  </joint>

</robot>
